# Odoo instance URL (http or https). No trailing slash.
ODOO_URL=https://your-odoo-instance.com

# Database name as shown in Odoo.
ODOO_DB=your-database-name

# Login: email or username.
ODOO_USER=your-username-or-email

# Password or API key.
ODOO_PASSWORD=your-password-or-api-key

# Set to true to verify SSL certificates (recommended in production).
ODOO_VERIFY_SSL=False

# Timeout in seconds for XML-RPC calls (default: 60). Between 1 and 3600.
ODOO_TIMEOUT=60
